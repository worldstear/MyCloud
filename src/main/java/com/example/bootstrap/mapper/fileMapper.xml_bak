<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.bootstrap.mapper.FileMapper">
    <!-- 删除数据 -->
    <select id="selectFileByUsername" resultType="com.example.bootstrap.pojo.FilePojo" parameterType="java.lang.String">
        select id,original_filename,upload_time from t_bootstrap_file_upload where upload_user = #{username}
    </select>
</mapper>